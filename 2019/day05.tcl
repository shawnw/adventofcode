#!/usr/bin/tclsh

source intcode.tcl

set testno 1
proc test {insns input expected_output} {
    global testno
    set prog [::intcode::compile $insns]
    set output [::intcode::execute $prog $input]
    if {$output == $expected_output} {
        puts "Test $testno: passed."
    } else {
        puts "test $testno: $orig failed. Expected output $expected_output got $output"
    }
    incr testno
}

